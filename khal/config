[calendars]

[[default]]
path = ~/.local/share/calendars/default
type = calendar

[default]
default_calendar = default

[locale]
local_timezone= Europe/Dublin
default_timezone= Europe/Dublin
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M
