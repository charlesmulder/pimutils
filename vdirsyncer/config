[general]
status_path = "~/.vdirsyncer/status/"

[pair my_cal]
a = "cm_cal"
b = "td_cal"
collections = ["from a", "from b"]

[storage cm_cal]
type = "filesystem"
path = "~/.local/share/calendars/"
fileext = ".ics"

[storage td_cal]
type = "caldav"
url = "https://nc.nl.tab.digital/remote.php/dav"
username = "charliemulder"
password.fetch = ["prompt", "Password for CalDAV"]

[pair my_contact]
